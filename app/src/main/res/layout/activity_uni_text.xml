        android:enabled="true"
        android:text="UNIText"
        android:textColor="@color/colorPrimary"
        app:uni_text_corner_radius="22.5dp"
        app:uni_text_end_color="@color/colorPrimary"
        app:uni_text_isHole="true"
        app:uni_text_start_color="@color/colorAccent" />

    <com.unicloud.core.view.text.UNIText
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:enabled="true"
        android:text="UNIText"
        android:textColor="@color/colorPrimary"

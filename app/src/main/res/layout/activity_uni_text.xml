        app:uni_text_corner_radius_br="10dp"
        app:uni_text_corner_radius_tl="10dp"
        app:uni_text_corner_radius_tr="10dp"
        app:uni_text_end_color="@color/colorPrimary"
        app:uni_text_isHole="true"
        app:uni_text_start_color="@color/colorAccent" />

